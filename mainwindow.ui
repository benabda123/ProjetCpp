}

/* Style pour la sélection (fond gris) */
QTableView::item:selected {
    background-color: #D3D3D3;  /* Couleur de fond pour l'élément sélectionné en gris */
    color: black;               /* Couleur du texte dans la sélection */
}

/* Appliquer un style à la première colonne */
QTableView::item:column(0) {
    background-color: red; /* Fond rouge pour la première colonne */
}
